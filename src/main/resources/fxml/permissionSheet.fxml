<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="300.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ib.PermissionsSheetController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backAction" text="Powrót do strony głownej" />
               </children>
            </HBox>
            <TableView fx:id="table" prefHeight="270.0" prefWidth="225.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="125.0" text="PESEL" />
                <TableColumn fx:id="name" prefWidth="250.0" text="Imię i nazwisko" />
                  <TableColumn fx:id="phone" prefWidth="125.0" text="Numer telefonu" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
